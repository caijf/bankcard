{
  "name": "bankcard",
  "version": "2.0.5",
  "description": "通过银行卡号查询银行和卡类型，支持浏览器端和node",
  "main": "dist/bankcard.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "npm run build:es && npm run build:lib && npm run build:umd",
    "build:es": "rm -rf es && tsc --outDir es -t es5 -m esnext -d",
    "build:lib": "rm -rf lib && tsc --outDir lib -m commonjs -d",
    "build:umd": "rm -rf dist && rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caijf/bankcard.git"
  },
  "keywords": [
    "bankcard",
    "bank",
    "cardType",
    "CardBin",
    "validateCardInfo",
    "searchCardBin"
  ],
  "author": "caijf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caijf/bankcard/issues"
  },
  "homepage": "https://github.com/caijf/bankcard#readme",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.1",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.3"
  }
}